{
  "name": "image_process_api",
  "version": "1.0.0",
  "description": "Image Processing API using Express & Typescript & Jasmine",
  "main": "index.ts",
  "keywords": [
    "API",
    "Typescript",
    "Express",
    "udacity"
  ],
  "scripts": {
    "prettier": "prettier --config .prettierrc './**/*.ts' --write",
    "lint": "eslint . --ext .ts;",
    "lint:f": "eslint . --ext .ts --fix",
    "jasmine": "jasmine",
    "build": "npx tsc",
    "format": "npm run lint:f && npm run prettier",
    "test": "npm run build && npm run jasmine",
    "dev": "nodemon ./src/index",
    "prod": "npm run build && node ./dist/index"
  },
  "author": "Abdullah Alakeel",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "sharp": "^0.31.1",
    "supertest": "^6.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.8.0",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
